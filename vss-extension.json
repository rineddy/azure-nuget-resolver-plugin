{
    "manifestVersion": 1,
    "id": "nuget-version-delivery-task",
    "name": "Nuget Version Delivery Task",
    "version": "0.9.1",
    "publisher": "eddy-r",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This task will resolve NuGet wildcard versions accoding to a 'prerelease/stable' flag",
    "categories": [
        "Azure Pipelines",
        "Build"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "nuget-version-delivery-task-id",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}